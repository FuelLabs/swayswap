{
  "name": "swayswap",
  "version": "0.1.0",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "homepage": "https://github.com/FuelLabs/swayswap#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FuelLabs/swayswap.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/FuelLabs/swayswap/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "ci:test-coverage": "run-s services:run contracts test:coverage services:clean",
    "ci:test": "run-s services:run contracts test services:clean",
    "contracts": "run-s contracts:*",
    "contracts:build": "pnpm run --filter swayswap-app contracts:build",
    "contracts:deploy": "pnpm run --filter swayswap-app contracts:deploy",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "pnpm lint:check --fix",
    "lint": "run-s lint:check prettier:check",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "services:clean": "make services-clean",
    "services:run": "make services-run",
    "test:coverage": "turbo run test -- --collectCoverage",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@fuels-ui/test-utils": "workspace:*",
    "@jest/types": "^28.1.0",
    "@swc/core": "^1.2.179",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.15.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^12.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.7",
    "turbo": "^1.2.8",
    "typescript": "^4.6.4"
  }
}
